{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "noImplicitAny": false,
    "suppressExcessPropertyErrors": true,
    "sourceMap": false,
    "noStrictGenericChecks": true,
    "outDir": "./dist",
    "declaration": false,
    "rootDir": "./src",
    "lib": [
      "dom",
      "es7"
    ],
    "noUnusedLocals": false
  },
  "exclude": [
    "./node_modules"
  ]
}